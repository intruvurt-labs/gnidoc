{
  "name": "gnidoc",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
  // 🚀 Universal App Control
  "start": "expo start --clear",
  "android": "expo run:android",
  "ios": "expo run:ios",
  "web": "expo start --web",

  // 🧩 Developer Tools
  "lint": "expo lint",
  "clean": "rm -rf node_modules .expo .expo-shared && npm install",
  "doctor": "expo doctor",

  // ⚡ Hotfixes / Router Patches
  "fix-router": "npx patch-package expo-router",
  "postinstall": "node patches/expo-router+use-patch.js",

  // 🌐 Web Dev Tunnel (via Rork Agent)
  "start-web-dev": "DEBUG=expo* npx rork start -p fd9jhdiphhthdxbi879dt --web --tunnel",

  // 🧱 Production Build Targets
  "build:web": "expo export:web",
  "build:android": "eas build --platform android",
  "build:ios": "eas build --platform ios",

  // 🧠 Security + Dependency Check
  "audit": "npm audit --production",
  "update": "npm update && npm dedupe && npm prune"
}

    "postinstall": "cd apps/mobile && npx patch-package && cd ../.. && prisma generate"
